<% content_for :body_content_left do %>
  <ul id="portfolio_items">
    <% @portfolio_items.each do |portfolio_item| %>
      <li>
        <%= link_to image_tag(portfolio_item.screenshot.thumbnail('300x400').url)+portfolio_item.title, portfolio_item_path(portfolio_item) %>
      </li>
    <% end %>
  </ul>
<% end %>

<%= render :partial => "/shared/content_page" %>
